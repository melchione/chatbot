{
	"name": "chatbot",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "NODE_OPTIONS=\"--inspect=0.0.0.0:9229\" vite dev --port 8080 --host 0.0.0.0 --log-level error",
		"clean": "rm -rf build || true",
		"build": "npm run clean && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"prettier": "prettier --write .",
		"prettier:check": "prettier --check .",
		"eslint": "eslint --fix .",
		"eslint:check": "eslint .",
		"lint": "npm run prettier:check && npm run eslint:check",
		"format": "npm run prettier && npm run eslint",
		"production": "node build"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@google-cloud/aiplatform": "^4.2.0"
	},
	"optionalDependencies": {
		"@rollup/rollup-android-arm-eabi": "4.22.4",
		"@rollup/rollup-android-arm64": "4.22.4",
		"@rollup/rollup-darwin-arm64": "4.22.4",
		"@rollup/rollup-darwin-x64": "4.22.4",
		"@rollup/rollup-linux-arm-gnueabihf": "4.22.4",
		"@rollup/rollup-linux-arm-musleabihf": "4.22.4",
		"@rollup/rollup-linux-arm64-gnu": "4.22.4",
		"@rollup/rollup-linux-arm64-musl": "4.22.4",
		"@rollup/rollup-linux-powerpc64le-gnu": "4.22.4",
		"@rollup/rollup-linux-riscv64-gnu": "4.22.4",
		"@rollup/rollup-linux-s390x-gnu": "4.22.4",
		"@rollup/rollup-linux-x64-gnu": "4.22.4",
		"@rollup/rollup-linux-x64-musl": "4.22.4",
		"@rollup/rollup-win32-arm64-msvc": "4.22.4",
		"@rollup/rollup-win32-ia32-msvc": "4.22.4",
		"@rollup/rollup-win32-x64-msvc": "4.22.4",
		"fsevents": "~2.3.2"
	},
	"overrides": {
		"vite": {
			"rollup": "npm:@rollup/wasm-node"
		}
	}
}